---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Hints from BinaryAge</title>
    <link href="http://hints.binaryage.com/atom.xml" rel="self"/>
    <link href="http://hints.binaryage.com"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://hints.binaryage.com/</id>
    
    {% for post in site.posts %}
    <entry>
        <title><![CDATA[{{ post.title }}]]></title>
        {% for category in post.categories %}
        <category term="{{category}}" scheme="http://www.sixapart.com/ns/types#tag" />
        {% endfor %}
        <link href="http://hints.binaryage.com/{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>hints.binaryage.com:{{ post.id }}</id>
        <content type="html"><![CDATA[{{ post.content }}]]></content>
        <author>
            <name>{{post.author_name}}</name>
            <url>{{post.author_url}}</url>
        </author>
    </entry>
    {% endfor %}
</feed>